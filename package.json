    "concurrently": "^5.0.0",
    "cypress": "^3.6.0",
    "eslint": "^6.6.0",
    "jest": "^24.9.0",
    "jest-preset-angular": "^8.0.0",
    "pre-commit": "^1.2.2",
    "rxjs-compat": "^6.5.3",
    "ts-jest": "^24.1.0",
    "ts-node": "~7.0.0",
    "typescript": "~3.5.3"
  }
