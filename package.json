{
  "name": "pttep-angular-starter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-dev": "npm run lint && npm run build",
    "build-prod": "npm run build -- --prod",
    "lint": "eslint \"src/**/*.ts\"",
    "lint-fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
